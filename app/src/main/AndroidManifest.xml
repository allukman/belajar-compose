<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.belajarcompose">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BelajarCompose"
        tools:targetApi="31">
        <activity
            android:name=".TimerCompose"
            android:exported="false"
            android:label="@string/title_activity_timer_compose"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".CircularProgressBar"
            android:exported="false"
            android:label="@string/title_activity_circular_progress_bar"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".SimpleAnimation"
            android:exported="false"
            android:label="@string/title_activity_simple_animation"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".Constraints"
            android:exported="false"
            android:label="@string/title_activity_constraints"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".List"
            android:exported="false"
            android:label="@string/title_activity_list"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".Snackbar"
            android:exported="false"
            android:label="@string/title_activity_snackbars"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".State"
            android:exported="false"
            android:label="@string/title_activity_state"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".StylingText"
            android:exported="false"
            android:label="@string/title_activity_styling_text"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".ImageCard"
            android:exported="false"
            android:label="@string/title_activity_image_card"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".Modifier"
            android:exported="false"
            android:label="@string/title_activity_modifier"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".RowColumn"
            android:exported="false"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.BelajarCompose" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.BelajarCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>